//5th iterated element in array
#include <stdio.h>
#include<stdio.h>
int main() {
    printf("Name: surya\nReg No:192324024\n");
    int n;
    printf("Enter the size:");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the elements:");
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    for(int i=0;i<n;i++){
    	if(i==4){
    		printf("5th element :%d",arr[i]);
    		break;
		}
	}
    return 0;
}
