#include<stdio.h>
int main(){
	printf("Name: surya\nReg No:192324024\n");
	int n,i,j;
	printf("Enter the size:");
	scanf("%d",&n);  
	int arr[n];
	printf("\nEnter the elements:");
	for(i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	printf("\nEnter the element to find:");
	scanf("%d",&j);
   	for(i=0;i<n;i++){
   		if(arr[i]==j){
   			printf("%d is found in index value %d ",j,i);
   			return 0;
		   }
	}
	printf("Element is not in the array");
	return 0;
}
