#include <stdio.h>
#include <string.h>
int main() {
    printf("Name: surya\nReg No:192324024\n");
    char str[100];
    printf("Enter String: ");
    scanf("%[^\n]s",str);
    int len=strlen(str);
    if(str[0]!='('&&str[len-1]!=')'){
    	printf("\nnot valid");
	}
	else
	printf("\nis valid");
    return 0;
}
