#include<stdio.h>
int main(){
	printf("Name: surya\nReg No:192324024\n");
	int array[50],index,i,n;
    printf("Enter the size:");
    scanf("%d",&n);
    printf("\nEnter the elements:");
    for (i=0;i<n;i++)    
        scanf("%d",&array[i]);
    printf("\nenter the element to delete:");
    scanf("%d",&index);
    for(i=index-1;i<n;i++)    
        array[i]=array[i+1];

    printf("\nResultant array is\n");
    for(i=0;i<=n-1;i++){
    	if(array[i]!='\0')
    	printf("%d ",array[i]);
	}    
            
    return 0;
}
