#include<stdio.h>

int main(){
	printf("Name: surya\nReg No:192324024\n");
	int n,i;
	printf("Enter the size:");
	scanf("%d",&n);  
	int arr[n];
	printf("\nEnter elements:");
	for(i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	printf("\nOdd numbers: ");
	for(i=0;i<n;i++){
		if(arr[i]%2!=0)
		printf("%d ",arr[i]);
	}
	printf("\nEven numbers: ");
	for(i=0;i<n;i++){
		if(arr[i]%2==0)
		printf("%d ",arr[i]);
	}
	return 0;
}
